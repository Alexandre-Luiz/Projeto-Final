<% if user_signed_in? %>
  <%= link_to 'Sair', destroy_user_session_path, method: :delete %>
<% end %>

<h1> Bem vindo ao marketplace <%= current_user.email %> </h1>


<% if :profile_complete? %>
  <h4> Complete seu perfil para poder negociar - <%= link_to 'Editar perfil', edit_user_registration_path(params[:id]) %></h4>
<% else %>
  <%= link_to 'Cadastrar produto', new_product_path %>
<% end%>

<div>
  <%= link_to 'Ver produtos', products_path %>
</div>

<div>
  <h5> nome: <%= current_user.name%> </h5>
</div>